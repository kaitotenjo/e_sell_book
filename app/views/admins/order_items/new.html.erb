<div class="main-panel">        
  <div class="content-wrapper">
    <div class="row">
      <div class="col-12 grid-margin stretch-card">
        <div class="card">
          <div class="card-body">
          <h1> order_items </h1>
              <%= form_with(url: admins_order_items_url) do |f|  %>
              <div class="form-group">
                  <label for="exampleInputName1">product id</label>
                  <%= f.text_field :product_id, autofocus: true , class:"form-control" ,placeholder:"price"%>
              </div>
              <div class="form-group">
                  <label for="exampleInputName1">Order ID</label>
                  <%= f.text_field :order_id, autofocus: true , class:"form-control",placeholder:"price"%>
              </div>
              <div class="form-group">
                <label for="exampleTextarea1">Quantity</label>
                <%= f.text_field :quantity,  class:"form-control" %>
              </div>
              <%= f.submit "submit" ,class: "btn btn-primary mr-2"%>
              <%= link_to "Cancel", admins_products_path,class:"btn btn-light" %>
              <%end%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>